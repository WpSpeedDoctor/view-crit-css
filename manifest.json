{
	"name": "View Crit CSS",
	"description": "Toggle external stylesheets to only see inline CSS",
	"version": "1.0.0",

	"background": {
		"scripts": [
			"background.js"
		]
	},

	"icons": {
		"48": "icons/48.png",
		"128": "icons/128.png"
	},

	"browser_action": {
		"default_icon": {
			"48": "icons/48.png",
			"128": "icons/128.png"
		}
	},

	"commands": {
	  "toggle-external-stylesheets": {
	    "suggested_key": {
	      "default": "Ctrl+E",
	      "mac": "Command+E"
	    },
	    "description": "Toggle external stylesheets"
	  }
	},

	"permissions": [
		"activeTab"
	],

	"manifest_version": 2
}
